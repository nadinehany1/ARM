#include "BIT_MATH.h"
#include "STD_TYPES.h"

#include "GPIO_Interface.h"
#include "RCC_Interface.h"
#include "STK_Interface.h"

#include "RTOS_Config.h"
#include "RTOS_Interface.h"
#include "RTOS_Private.h"
void APP_LED1(void);
void APP_LED2(void);
void APP_LED3(void);


void APP_LED1(void)
{
	static u8 Local_u8BitValue;
	TOG_BIT(Local_u8BitValue,0);
	GPIO_voidSetPinValue(GPIOA,0,Local_u8BitValue);
}
void APP_LED2(void)
{
	static u8 Local_u8BitValue;
	TOG_BIT(Local_u8BitValue,0);
	GPIO_voidSetPinValue(GPIOA,2,Local_u8BitValue);
}
void APP_LED1(void)
{
	static u8 Local_u8BitValue;
	TOG_BIT(Local_u8BitValue,0);
	GPIO_voidSetPinValue(GPIOA,0,Local_u8BitValue);
}
