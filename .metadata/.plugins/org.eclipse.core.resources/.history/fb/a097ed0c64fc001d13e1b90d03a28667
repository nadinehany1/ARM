#include "FreeRTOS.h"
#include "task.h"
void Task1(void)
{
	static u8 flag = 0;
	while(1)
	{
		if(flag==0)
		{
			GPIO_voidSetPinValue(GPIOA,PIN0,1);
			flag=1;
		}
	}
}
int main()
{
  // At this stage the system clock should have already been configured
  // at high speed.

  // Infinite loop
  while (1)
    {
       // Add your code here.
    }
  return 0;
}


